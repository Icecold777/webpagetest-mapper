<?xml version="1.0" encoding="utf-8"?>
<document
 xmlns="urn:oasis:names:tc:opendocument:xmlns:office:1.0"
 xmlns:mt="urn:oasis:names:tc:opendocument:xmlns:meta:1.0"
 xmlns:dc="http://purl.org/dc/elements/1.1/"
 xmlns:st="urn:oasis:names:tc:opendocument:xmlns:style:1.0"
 xmlns:tb="urn:oasis:names:tc:opendocument:xmlns:table:1.0"
 xmlns:of="urn:oasis:names:tc:opendocument:xmlns:of:1.2"
 xmlns:fo="urn:oasis:names:tc:opendocument:xmlns:xsl-fo-compatible:1.0"
 mimetype="application/vnd.oasis.opendocument.spreadsheet"
 version="1.2"
>
    <meta>
        <mt:generator>{{application}}/{{version}}</mt:generator>
        <dc:title>WebPageTest results, {{date}}</dc:title>
    </meta>
    <automatic-styles>
        <st:style st:name="table" st:family="table">
            <st:table-properties tb:display="true" st:writing-mode="lr-tb"/>
        </st:style>
        <st:style st:name="label-column" st:family="table-column">
            <st:table-column-properties fo:break-before="auto" st:column-width="1.2in"/>
        </st:style>
        <st:style st:name="value-column" st:family="table-column">
            <st:table-column-properties fo:break-before="auto" st:column-width="1in"/>
        </st:style>
        <st:style st:name="row" st:family="table-row">
            <st:table-row-properties
             fo:break-before="auto"
             st:row-height="0.2in"
             st:use-optimal-row-height="true"
            />
        </st:style>
        <st:style st:name="common-cell" st:family="table-cell">
            <st:table-cell-properties st:vertical-align="middle"/>
            <st:text-properties st:font-name="Arial" st:font-size="10pt"/>
        </st:style>
        <st:style st:name="label-cell" st:family="table-cell" st:parent-style-name="common-cell">
            <st:text-properties fo:font-weight="bold"/>
            <st:paragraph-properties fo:text-align="center"/>
        </st:style>
        <st:style st:name="value-cell" st:family="table-cell" st:parent-style-name="common-cell">
            <st:text-properties fo:font-weight="normal"/>
        </st:style>
    </automatic-styles>
    <body>
        <spreadsheet>
            <tb:label-ranges>
                <tb:label-range
                 tb:label-cell-range-address="A1:I2"
                 tb:data-cell-range-address="A3:I{{add results.length 2}}"
                 tb:orientation="column"
                />
                <tb:label-range
                 tb:label-cell-range-address="A{{add results.length 4}}:A{{add results.length 15}}"
                 tb:data-cell-range-address="B{{add results.length 4}}:I{{add results.length 15}}"
                 tb:orientation="row"
                />
            </tb:label-ranges>
            <tb:table tb:style-name="table">
                <tb:table-header-columns>
                    <tb:table-column tb:style-name="label-column"/>
                </tb:table-header-columns>
                <tb:table-columns>
                    <tb:table-column
                     tb:number-columns-repeated="8"
                     tb:style-name="value-column"
                    />
                </tb:table-columns>
                <tb:table-header-rows>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell/>
                        <tb:table-cell
                         tb:number-columns-spanned="4"
                         tb:style-name="label-cell"
                         string-value="First view"
                        />
                        <tb:covered-table-cell tb:number-columns-repeated="3"/>
                        <tb:table-cell
                         tb:number-columns-spanned="4"
                         tb:style-name="label-cell"
                         string-value="Repeat view"
                        />
                        <tb:covered-table-cell tb:number-columns-repeated="3"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell/>
                        <tb:table-cell tb:style-name="label-cell" string-value="First byte"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="Start render"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="Load"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="Speed index"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="First byte"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="Start render"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="Load"/>
                        <tb:table-cell tb:style-name="label-cell" string-value="Speed index"/>
                    </tb:table-row>
                </tb:table-header-rows>
                <tb:table-rows>
{{#each results}}                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="{{id}}"/>
{{#each metrics}}                        <tb:table-cell tb:style-name="value-cell" value="{{value}}"/>
{{/each}}                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:number-columns-repeated="9"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="UPPER"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.B3:.B{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.C3:.C{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.D3:.D{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.E3:.E{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.F3:.F{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.G3:.G{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.H3:.H{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MAX([.I3:.I{{add results.length 2}}])"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="LOWER"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.B3:.B{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.C3:.C{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.D3:.D{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.E3:.E{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.F3:.F{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.G3:.G{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.H3:.H{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MIN([.I3:.I{{add results.length 2}}])"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:number-columns-repeated="9"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="90th PCT"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.B3:.B{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.C3:.C{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.D3:.D{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.E3:.E{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.F3:.F{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.G3:.G{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.H3:.H{{add results.length 2}}];0.9)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.I3:.I{{add results.length 2}}];0.9)"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="10th PCT"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.B3:.B{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.C3:.C{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.D3:.D{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.E3:.E{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.F3:.F{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.G3:.G{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.H3:.H{{add results.length 2}}];0.1)"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=PERCENTILE([.I3:.I{{add results.length 2}}];0.1)"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:number-columns-repeated="9"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="MEAN"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.B3:.B{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.C3:.C{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.D3:.D{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.E3:.E{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.F3:.F{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.G3:.G{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.H3:.H{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=AVERAGE([.I3:.I{{add results.length 2}}])"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="STDEV"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.B3:.B{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.C3:.C{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.D3:.D{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.E3:.E{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.F3:.F{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.G3:.G{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.H3:.H{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=STDEV([.I3:.I{{add results.length 2}}])"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:number-columns-repeated="9"/>
                    </tb:table-row>
                    <tb:table-row tb:style-name="row">
                        <tb:table-cell tb:style-name="value-cell" string-value="MEDIAN"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.B3:.B{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.C3:.C{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.D3:.D{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.E3:.E{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.F3:.F{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.G3:.G{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.H3:.H{{add results.length 2}}])"/>
                        <tb:table-cell tb:style-name="value-cell" tb:formula="of:=MEDIAN([.I3:.I{{add results.length 2}}])"/>
                    </tb:table-row>
{{/each}}                </tb:table-rows>
            </tb:table>
        </spreadsheet>
    </body>
</document>

